{
    "extends": [
        "react-app",
        "react-app/jest"
      ],
    "rules": {
        "no-unused-vars":"warn",
        "no-console":"warn",
        "quotes": ["warn", "single"],
        "indent": ["warn", 4],
        "max-len": ["warn", {"code" : 200, "ignoreUrls": true}],
        "import/order": ["warn", 
            {
                "groups": ["builtin", "external", "internal", "parent", "sibling", "index", "object", "type"],
                "newlines-between": "always-and-inside-groups"
            }
        ]
    }  
}